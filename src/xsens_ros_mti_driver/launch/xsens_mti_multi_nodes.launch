<launch>
    <!-- Use groups if you want to launch several nodes for multiple devices.
    Make sure to use different param files for each sensor. -->
    <group ns="MTi_300">
        <node  name="xsens_mti_node300" pkg="xsens_mti_driver" type="xsens_mti_node" output="screen">
            <rosparam command="load" file="$(find xsens_mti_driver)/param/xsens_mti_node_MTi-300.yaml" />
        </node>
    </group>
    <group ns="MTi_680G">
        <node  name="xsens_mti_node680G" pkg="xsens_mti_driver" type="xsens_mti_node" output="screen">
            <rosparam command="load" file="$(find xsens_mti_driver)/param/xsens_mti_node_MTi-680G.yaml" />
        </node>
    </group>
</launch>
